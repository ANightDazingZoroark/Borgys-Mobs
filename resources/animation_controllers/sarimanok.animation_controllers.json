{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.sarimanok_movement": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						{
							"animIdle": "query.ground_speed == 0 && query.is_on_ground"
						},
						{
							"animWalk": "query.ground_speed > 0 && query.is_on_ground"
						},
						{
							"animFall": "!query.is_on_ground"
						}
					]
				}
			}
		}
    }
}