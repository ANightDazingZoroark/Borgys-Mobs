{
    "format_version": "1.16.0",
    "minecraft:entity": {
        "description": {
            "identifier": "borgy:fast_headcrab_zombie",
            "is_spawnable": true,
            "is_summonable": true,
            "is_experimental": false
        },
        "component_groups": {
            "borgy:default_targets": {
                "minecraft:behavior.nearest_attackable_target": {
                    "priority": 3,
                    "within_radius": 24,
                    "reselect_targets": true,
                    "entity_types": [
                        {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "value": "mob"
                                    },
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "operator": "!=",
                                        "value": "headcrabGeneral"
                                    },
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "operator": "!=",
                                        "value": "headcrab_zombie_general"
                                    },
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "operator": "!=",
                                        "value": "monster"
                                    }
                                ]
                            },
                            "max_dist": 24
                        },
                        {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "value": "player"
                                    },
                                    {
                                        "test": "is_riding",
                                        "subject": "other",
                                        "value": false
                                    }
                                ]
                            },
                            "max_dist": 24
                        }
                    ]
                }
            },
            "borgy:attack_forced_attack_entity": {
                "minecraft:behavior.nearest_attackable_target": {
                    "priority": 3,
                    "within_radius": 24,
                    "reselect_targets": true,
                    "entity_types": [
                        {
                            "filters": {
                              "test": "is_family",
                              "subject": "other",
                              "value": "isForcedAttacking"
                            },
                            "max_dist": 24
                        }
                    ]
                },
                "minecraft:on_target_escape": {
                    "event": "borgy:no_force_attacked",
                    "target": "self"
                }
            },
			"borgy:drop_experience_from_forced_attack": {
			  	"minecraft:experience_reward": {
					"on_death": "Math.random(3,6)"
			  	}
			}
        },
        "components": {
            "minecraft:health": {
                "value": 12
            },
            "minecraft:attack": {
                "damage": 10
            },
            "minecraft:breathable": {
				"total_supply": 15,
				"suffocate_time": 0
			},
            "minecraft:collision_box": {
                "width": 0.6,
                "height": 1.9
            },
            "minecraft:movement": {
                "value": 0.35
            },
            "minecraft:type_family": {
                "family": [
                    "fast_headcrab_zombie",
                    "headcrab_zombie_general",
                    "mob"
                ]
            },
            "minecraft:physics": {},
            "minecraft:pushable": {},
			"minecraft:navigation.climb": {
				"avoid_water": true,
				"avoid_damage_blocks": true
			},
			"minecraft:movement.basic": {},
            "minecraft:jump.static": {},
            "minecraft:behavior.random_stroll": {
                "priority": 6
            },
            "minecraft:behavior.stalk_and_pounce_on_target": {
                "priority": 4,
                "stalk_speed": 1.2,
                "max_stalk_dist": 12.0,
                "leap_height": 0.9,
                "leap_dist": 1.5,
                "pounce_max_dist": 11,
                "interest_time": 0,
                "strike_dist": 8
            },
			"minecraft:behavior.delayed_attack": {
				"priority": 5,
				"attack_duration": 0.64,
				"hit_delay_pct": 0.8125,
				"track_target": true,
				"reach_multiplier": 1.5
			},
            "minecraft:behavior.hurt_by_target": {
				"priority": 2,
                "entity_types": {
                    "filters": {
                        "all_of": [
                            {
                                "test": "is_family",
                                "subject": "other",
                                "operator": "!=",
                                "value": "headcrabGeneral"
                            },
                            {
                                "test": "is_family",
                                "subject": "other",
                                "operator": "!=",
                                "value": "headcrab_zombie_general"
                            },
                            {
                                "test": "is_family",
                                "subject": "other",
                                "operator": "!=",
                                "value": "sigbin"
                            },
                            {
                                "test": "is_family",
                                "subject": "other",
                                "operator": "!=",
                                "value": "forcedAttack"
                            }
                        ]
                    }
                }
			},
            "minecraft:behavior.avoid_mob_type": {
                "priority": 1,
                "entity_types": [
                    {
                        "filters": {
                            "any_of": [
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "sigbin"
                                },
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "parasaurScary"
                                }
                            ]
                        },
                        "max_dist": 10,
                        "walk_speed_multiplier": 1.5,
                        "sprint_speed_multiplier": 1.5
                    }
                ]
            },
            "minecraft:behavior.random_look_around": {
                "priority": 5
            },
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:nameable": {},
            "minecraft:experience_reward": {
                "on_death": "query.last_hit_by_player ? Math.Random(3,6) : 0"
            },
            "minecraft:despawn": {
                "despawn_from_distance": {}
            },
            "minecraft:damage_sensor": {
                "triggers": [
                    {
                        "on_damage": {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "has_damage",
                                        "subject": "self",
                                        "value": "fatal"
                                    },
									{
                                        "test": "is_family",
                                        "subject": "other",
                                        "operator": "!=",
                                        "value": "forcedAttack"
                                    }
                                ]
                            },
                            "event": "borgy:leave_headcrab_behind",
                            "target": "self"
                        }
                    },
					{
						"cause": "fall",
						"deals_damage": false
					},
                    {
						"on_damage": {
							"filters": {
								"all_of": [
									{
								  		"test": "is_family",
								  		"subject": "other",
								  		"value": "forcedAttack"
									},
									{
								  		"test": "has_target",
								  		"subject": "self",
								  		"value": false
									},
									{
									  	"test": "has_damage",
									  	"subject": "self",
									  	"operator": "!=",
									  	"value": "fatal"
									}
							  	]
							},
							"event": "borgy:on_force_attacked",
							"target": "self"
						}
					},
					{
						"on_damage": {
							"filters": {
						  		"all_of": [
									{
							  			"test": "is_family",
							  			"subject": "other",
							  			"value": "forcedAttack"
									},
									{
							  			"test": "has_damage",
							  			"subject": "self",
							  			"value": "fatal"
									}
						  		]
							},
							"event": "borgy:on_force_attacked_death",
							"target": "self"
					  	}
					},
					{
					  	"on_damage": {
							"filters": {
						  		"test": "is_family",
						  		"subject": "other",
						  		"value": "sarcoSpinAttack"
							},
							"event": "borgy:hit_by_sarco_spin",
							"target": "self"
					  	}
					},
					{
						"on_damage": {
						  	"filters": {
								"test": "has_equipment",
								"subject": "other",
								"value": "rift:dimetrodon_frost_attack"
						  	},
						  	"event": "borgy:hit_by_frost_dimetrodon",
						  	"target": "self"
						}
					}
                ]
            }
        },
        "events": {
            "minecraft:entity_spawned": {
                "add": {
                    "component_groups": [
                        "borgy:default_targets"
                    ]
                }
            },
            "borgy:leave_headcrab_behind": {
                "run_command": {
                    "command": [
                        "summon borgy:fast_headcrab"
                    ]
                }
            },
			"borgy:on_force_attacked": {
				"add": {
				  	"component_groups": [
						"borgy:attack_forced_attack_entity"
				  	]
				},
				"remove": {
				  	"component_groups": [
						"borgy:default_targets"
				  	]
				}
			},
			"borgy:no_force_attacked": {
                "add": {
                    "component_groups": [
                        "borgy:default_targets"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "borgy:attack_forced_attack_entity"
                    ]
                }
			},
			"borgy:hit_by_sarco_spin": {
				"run_command": {
					"command": [
						"effect @s slowness 30 2"
					]
				}
			},
			"borgy:hit_by_frost_dimetrodon": {
				"run_command": {
					"command": [
						"effect @s weakness 20 2",
						"effect @s slowness 20 2"
				  	]
				}
			},
			"borgy:on_force_attacked_death": {
			  	"add": {
					"component_groups": [
				  		"borgy:drop_experience_from_forced_attack"
					]
			  	},
                "run_command": {
                    "command": [
                        "summon borgy:fast_headcrab"
                    ]
                }
			}
        }
    }
}