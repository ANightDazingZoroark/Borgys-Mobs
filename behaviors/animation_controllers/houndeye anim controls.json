{
    "format_version": "1.16.0",
    "animation_controllers": {
        "controller.animation.houndeye_pack": {
            "initial_state": "default",
            "states": {
                "default": {
                    "transitions": [
                        {
                            "is_wild": "!query.is_tamed"
                        }
                    ]
                },
                "is_wild": {
                    "animations": [
                        "animPackDetection"
                    ],
                    "transitions": [
                        {
                            "default": "query.is_tamed"
                        },
                        {
                            "one": "v.x == 0"
                        },
                        {
                            "two": "v.x == 1"
                        },
                        {
                            "three": "v.x == 2"
                        },
                        {
                            "four": "v.x == 3"
                        }
                    ]
                },
                "one": {
                    "animations": [
                        "animPackDetection"
                    ],
                    "on_entry": [
                        "@s borgy:one_houndeye",
                        "/say one houndeye"
                    ],
                    "transitions": [
                        {
                            "default": "query.is_tamed"
                        },
                        {
                            "two": "v.x == 1"
                        },
                        {
                            "three": "v.x == 2"
                        },
                        {
                            "four": "v.x == 3"
                        }
                    ]
                },
                "two": {
                    "animations": [
                        "animPackDetection"
                    ],
                    "on_entry": [
                        "@s borgy:two_houndeyes",
                        "/say two houndeyes"
                    ],
                    "transitions": [
                        {
                            "default": "query.is_tamed"
                        },
                        {
                            "one": "v.x == 0"
                        },
                        {
                            "three": "v.x == 2"
                        },
                        {
                            "four": "v.x == 3"
                        }
                    ]
                },
                "three": {
                    "animations": [
                        "animPackDetection"
                    ],
                    "on_entry": [
                        "@s borgy:three_houndeyes",
                        "/say three houndeyes"
                    ],
                    "transitions": [
                        {
                            "default": "query.is_tamed"
                        },
                        {
                            "one": "v.x == 0"
                        },
                        {
                            "two": "v.x == 1"
                        },
                        {
                            "four": "v.x == 3"
                        }
                    ]
                },
                "four": {
                    "animations": [
                        "animPackDetection"
                    ],
                    "on_entry": [
                        "@s borgy:four_houndeyes",
                        "/say four houndeyes"
